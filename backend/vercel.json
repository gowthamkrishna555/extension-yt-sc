{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/server.js",
      "methods": ["GET", "POST", "OPTIONS"]
    }
  ],
  "functions": {
    "server.js": {
      "memory": 1024,
      "maxDuration": 60
    }
  }
}